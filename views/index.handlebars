                </div>                
            </div>
            <div class="row">
                {{#each content.fields.section5.fields.resources}}
                <div class="col-md-6">
                    <div class="row justify-content-center">
                        <marked class="description">{{this.fields.imageTitle}}</marked>
                    </div>
                    <div class="row justify-content-center">
