<div class="row">
    <section id="section1" class="section" style="background-image: url('{{content.fields.sectionBackground.fields.file.url}}'); background-size: cover;">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <marked class="description">{{content.fields.section1.fields.description}}</marked>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-10 offset-xs-1 col-sm-10 offset-sm-1 col-md-8 offset-md-2 col-lg-6 offset-lg-3">
                    <img src="{{content.fields.section1.fields.image1.fields.file.url}}" style="width: 100%; height: auto;" />
                </div>
            </div>
        </div>
    </section>
</div>

